{
  "extends": "eslint:recommended",
  "env": {
    "node": true
  },
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "comma-dangle": ["error", "always-multiline"]
  }
}
